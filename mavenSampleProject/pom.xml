	<build>
		<finalName>mavenSampleProject</finalName>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-maven-plugin</artifactId>
					<version>2.7.0</version>
					<configuration>
						<includeSystemScope>true</includeSystemScope>
						<!--                    <outputDirectory>${project.basedir}/../output/${project.artifactId}</outputDirectory>-->
						<mainClass>org.apache.myfaces.MavenApp</mainClass>
						<!--                    <fork>true</fork>-->
					</configuration>
					<executions>
						<execution>
							<id>repackage</id>
							<phase>verify</phase>
							<goals>
								<goal>repackage</goal>
							</goals>
							<!--<configuration>
